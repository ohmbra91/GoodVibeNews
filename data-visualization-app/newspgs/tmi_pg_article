        ads: {
            'skipAd': {
                'enabled': false,
                'timeOut': 5
            },
            'servers': [
              {
                  'apiAddress': 'https://ads.independent.com.mt/www/delivery/fc.php?script=bannerTypeHtml:vastInlineBannerTypeHtml:vastInlineHtml&format=vast&nz=1&zones=pre-roll%3D84'
