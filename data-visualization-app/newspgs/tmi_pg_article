            ],
            'schedule': [
              {
                  'position': 'pre-roll'
              }
            ]
        }
    };

    var p2 = _V_("player2", optionsAdvert);
	p2.muted(true);
    var playerStarted = false;

    $(window).scroll(function () {
        var top_of_element = $("#p2Container").offset().top;
        var bottom_of_element = $("#p2Container").offset().top + $("#p2Container").outerHeight();
        var bottom_of_screen = $(window).scrollTop() + $(window).height();
        if ((bottom_of_screen > top_of_element) && (bottom_of_screen < bottom_of_element)) {
            //start player
            if (!playerStarted) {
                playerStarted = true;
                p2.play();
				p2.pause();
            }
        }
    });



